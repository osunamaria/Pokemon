<%- include("template/cabecera",{tituloWeb:"Crear"}) %>
    <div class="container">
        <h1>Crear nuevo entrenador</h1>
        <form action="/entrenador" class="my-2" method="POST">
            <input type="text" placeholder="Nombre entrenador" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Compañero" class="form-control my-2" name="companero">
            <input type="text" placeholder="Origen entrenador" class="form-control my-2" name="origen">
            <input type="text" placeholder="Contraseña" class="form-control my-2" name="contrasena">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
